<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
<h1 class="h2">Gerenciar</h1>
<div class="btn-toolbar mb-2 mb-md-0">
  <%= link_to "Adicionar", new_municipe_path, class: 'btn btn-sm btn-outline-secondary' %>
</div>
</div>

<div id="municipes">
  <% @municipes.each do |municipe| %>
    <%= render municipe %>
    <p>
      <%= link_to "Show this municipe", municipe %>
    </p>
  <% end %>
</div>